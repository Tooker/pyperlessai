# Example .env for pyperless-ai PoC
# Copy this to .env and fill in real values (do NOT commit secrets).

# Paperless-ngx
PAPERLESS_BASE_URL=https://paperless.example
PAPERLESS_API_TOKEN=your_paperless_token_here

# OpenAI
OPENAI_API_KEY=sk-your-openai-key
OPENAI_MODEL=gpt-4o-mini-vision

# Image / concurrency settings
MAX_IMAGE_SIZE=1024
CONCURRENT_WORKERS=4

# Logging
LOG_LEVEL=INFO
